/* ************************************顶部导航栏 */
.navbar.data-v-6e2ebf8a {
	position: -webkit-sticky;
	position: sticky;
	top: 0;
	z-index: 1000;
}
.left-icon.data-v-6e2ebf8a {
	position: absolute;
	left: 10px;
	top: 8%;
	-webkit-transform: translateY(-50%);
	        transform: translateY(-50%);
